# game/events/olivia.rpy:589
translate italian olivia_repeatable_night_out_with_0f6c9465:

    # Lisa_02 "Макс очень внимательный, когда хочет за кем-нибудь подсмотреть!"
    Lisa_02 "Max è molto attento quando vuole spiare qualcuno!"

# game/events/olivia.rpy:590
translate italian olivia_repeatable_night_out_with_d1813384:

